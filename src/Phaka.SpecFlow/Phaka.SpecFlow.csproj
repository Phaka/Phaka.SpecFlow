    <PackageLicenseUrl>https://github.com/Phaka/Phaka.SpecFlow/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Phaka/Phaka.SpecFlow</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Phaka/Phaka.SpecFlow</RepositoryUrl>
    <RepositoryType>GIT</RepositoryType>
    <PackageTags>SpecFlow</PackageTags>
    <Authors>Werner Strydom</Authors>
    <Company>Werner Strydom</Company>
    <Product>Phaka</Product>
